if __name__ == '__main__':
    arr = []
    for _ in range(int(input())):
        name = input()
        score = float(input())
        arr.append([name,score])
    arr.sort(key= lambda x : x[1])
    secLow = arr[-2][1]
    nameSort = sorted(arr, key= lambda x : x[0])
    for name, score in nameSort:
        if score == secLow:
            print(name)

    
    
        